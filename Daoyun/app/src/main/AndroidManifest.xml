    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Daoyun">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ForgetPasswordActivity"></activity>
        <activity android:name=".RigesterActivity"></activity>
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".HomeControllerActivity"></activity>
        <activity android:name=".ProjectDetailActivity"></activity>
        <activity android:name=".MemberActvity"></activity>
        <activity android:name=".MemberDetailActivity"></activity>
        <activity android:name=".SignInActivity"></activity>
        <activity android:name=".CreateProjectActivity"></activity>
        <activity android:name=".AboutMeActivity"></activity>
        <activity android:name=".AboutMeDetailActivity"></activity>
    </application>

</manifest>