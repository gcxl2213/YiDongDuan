<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daoyun">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/home_selector" />

        <activity
            android:name=".HomeManagerActivity"
            android:label="@string/title_activity_home_manager" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".RigesterActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".HomeControllerActivity" />
        <activity android:name=".ProjectDetailActivity" />
        <activity android:name=".MemberActvity" />
        <activity android:name=".MemberDetailActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".CreateProjectActivity" />
        <activity android:name=".AboutMeDetailActivity" />
    </application>

</manifest>